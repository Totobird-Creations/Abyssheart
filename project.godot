config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "Entity",
"language": "GDScript",
"path": "res://main/game/entity/entity.gd"
}, {
"base": "Entity",
"class": "PlayerEntity",
"language": "GDScript",
"path": "res://main/game/entity/player.gd"
} ]
_global_script_class_icons={
"Entity": "",
"PlayerEntity": ""
}

[application]

config/name="Abyssheart"
config/description="A survival game, taking place within a cave."
run/main_scene="res://main/boot/boot.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="totobird_creations.abyssheart"
boot_splash/show_image=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Logger="*res://main/global/logger.gd"
PostProcessing="*res://main/global/post_processing.tscn"
DiscordLink="*res://main/global/discord_link.tscn"

[debug]

gdscript/warnings/return_value_discarded=false
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=4096
window/size/height=2400
window/size/test_width=1024
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="expand"

